    "fs-extra": "^10.0.0",
    "google-protobuf": "^3.21.4",
    "grpc-web": "^1.5.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
